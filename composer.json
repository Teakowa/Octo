{
    "name": "teakowa/octo",
    "description": "A composer package to simple use music service API.",
    "type": "library",
    "keywords": [
        "php music",
        "music api",
        "kugou",
        "tencent"
    ],
    "support": {
        "issues": "https://github.com/teakowa/octo/issues",
        "source": "https://github.com/teakowa/octo"
    },
    "require": {
        "php": ">=7.1.3",
        "ext-bcmath": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "guzzlehttp/guzzle": "^6.2",
        "guzzlehttp/psr7": "^1.6",
        "metowolf/meting": "^1.5",
        "psr/http-message": "~1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.15",
        "phpmd/phpmd": "@stable",
        "phpstan/phpstan": "^0.12.19",
        "phpunit/phpunit": "^7.5 || ^8.0"
    },
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Teakowa",
            "email": "contact@teakowa.me",
            "homepage": "https://teakowa.me",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Teakowa\\Octo\\": "src"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
